{
  "name": "outline-mcp",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "scripts": {
    "dev:docs": "pnpm --filter @outline-mcp/docs dev",
    "dev:mcp": "pnpm --filter @outline-mcp/mcp dev",
    "build": "pnpm --filter @outline-mcp/docs build && pnpm --filter @outline-mcp/mcp build",
    "lint": "pnpm --filter @outline-mcp/docs lint && pnpm --filter @outline-mcp/mcp lint",
    "typecheck": "pnpm --filter @outline-mcp/docs types:check && pnpm --filter @outline-mcp/mcp typecheck",
    "test": "pnpm --filter @outline-mcp/mcp test",
    "e2e:mcp": "pnpm --filter @outline-mcp/mcp e2e:inspector"
  }
}
